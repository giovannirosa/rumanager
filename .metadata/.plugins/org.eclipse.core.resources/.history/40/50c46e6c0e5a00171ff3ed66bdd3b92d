package model;

public class Ingrediente {
	
	private int qtde;
	private String unidade;
	
	ItemEstoque it;
	
	public Ingrediente(int qtde, String unidade) {
		super();
		this.qtde = qtde;
		this.unidade = unidade;
	}

	public int getQtde() {
		return qtde;
	}

	public void setQtde(int qtde) {
		this.qtde = qtde;
	}

	public String getUnidade() {
		return unidade;
	}

	public void setUnidade(String unidade) {
		this.unidade = unidade;
	}
}
